{
  "diagnostic.refreshOnInsertMode": true, // activates diagnostics on insert mode
  "clangd.semanticHighlighting": true,

  "snippets.priority": 1,
  "coc.source.file.priority": 2,
  "coc.source.around.priority": 3,
  "coc.source.buffer.priority": 4,
  "suggest.languageSourcePriority": 99,

  // explorer
  "explorer.width": 30,
  "explorer.buffer.root.template": "[icon & 1] OPEN EDITORS",
  "explorer.file.root.template": "[icon & 1] PROJECT ([root])",
  "explorer.file.showHiddenFiles": true,
  "explorer.file.autoReveal": false,
  /* "explorer.icon.enableVimDevicons": true, */
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "S": "open:vsplit",
    "V": "open:ssplit",
    "mr": "rename",
    "mc": "cutFile",
    /* "mC": "cutFile", */
    "C": "copyFile",
    "md": "delete",
    "D": "delete",
    "ma": "addFile",
    "mA": "addDirectory"
  },
  "explorer.git.icon.status.modified": "\u2739",
  "explorer.git.icon.status.untracked": "\u272D",
  "explorer.git.icon.status.ignored": "\u0021",

  "coc.preferences.snippetStatusText": " ",
  "suggest.snippetIndicator": " \ue796",
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf6a6",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  /* "explorer.git.icon.status.ignored": "\u2757", */

  // use prettier for formatting
  "prettier.formatterPriority": 1,
  /*"prettier.singlequote": true,
  "prettier.eslintintegration": true,
  "prettier.disablesuccessmessage": true,
  "prettier.disablelanguages": [],*/

  // codelens
  "codeLens.enable": true,
  "java.referencesCodeLens.enabled": true,
  "java.implementationsCodeLens.enabled": true,
  "java.completion.enabled": true,

  // autoformat
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "json",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "typescript.tsx",
    "graphql",
    "html",
    "yaml",
    "json",
    "python",
    "c",
    "cpp",
    "java",
    "scala"
  ],

  "clangd.path": "/Users/mgokbulut/.config/coc/extensions/coc-clangd-data/install/11.0.0/clangd_11.0.0/bin/clangd"

